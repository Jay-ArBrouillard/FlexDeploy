        "type": "error",
        "displayMode": "persist"
      }
    },
    "callRestGet1113185HcmCountriesLovQ": {
      "module": "vb/action/builtin/restAction",
      "parameters": {
        "endpoint": "oracleHcmEndpoints/getHcmCountriesLovQ",
        "uriParams": {
          "limit": "{{ \"500\" }}",
          "q": "{{ \"ObsoleteFlag=false;CountryName LIKE \\\"%\" + $page.variables.SearchPhoneCountryVar6 + \"\\\"%\" }}"
        }
      },
