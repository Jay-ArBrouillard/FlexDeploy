{
  "description": "",
  "root": "resetVariablesGetHcmPhoneCountryCodeADP6",
  "actions": {
    "fireNotification": {
      "module": "vb/action/builtin/fireNotificationEventAction",
      "parameters": {
        "target": "leaf",
